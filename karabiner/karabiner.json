                "rules": [
                    {
                        "description": "Change CapsLock to Command-Shift-Option-Control modifier (or Escape when pressed alone)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock"
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "hyper_modifier",
                                            "value": 1
                                        }
                                    },
                                    {
                                        "key_code": "left_shift",
                                        "lazy": true,
                                        "modifiers": [
                                            "left_command",
                                            "left_control",
                                            "left_option"
                                        ]
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "hyper_modifier",
                                            "value": 0
                                        }
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "escape"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Disable Command-Shift-Option-Control-Period (.)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "period",
                                    "modifiers": {
                                        "mandatory": [
                                            "command",
                                            "shift",
                                            "option",
                                            "control"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "escape"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Disable Command-Shift-Option-Control-Comma (,)",
